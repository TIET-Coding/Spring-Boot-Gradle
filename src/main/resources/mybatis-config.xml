<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <!-- Other configurations like settings, mappers, etc. -->

    <!-- Define type aliases for domain classes -->
    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer"/>
        <typeAlias alias="Long" type="java.lang.Long"/>
        <typeAlias alias="HashMap" type="java.util.HashMap"/>
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap"/>
        <typeAlias alias="LinkedList" type="java.util.LinkedList"/>
        <typeAlias alias="Users" type="com.myspringboot.demo.model.User"/>
        <typeAlias alias="Masters" type="com.myspringboot.demo.model.Master"/>
        <typeAlias alias="Roleuser" type="com.myspringboot.demo.model.RoleUser"/>
        <typeAlias alias="Permissions" type="com.myspringboot.demo.model.Permissions"/>

    </typeAliases>

    <!-- Other settings go here -->
</configuration>
