<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <!-- Other configurations like settings, mappers, etc. -->

    <!-- Define type aliases for domain classes -->
    <typeAliases>

        <typeAlias alias="Integer" type="java.lang.Integer"/>
        <typeAlias alias="Long" type="java.lang.Long"/>
        <typeAlias alias="HashMap" type="java.util.HashMap"/>
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap"/>
        <typeAlias alias="LinkedList" type="java.util.LinkedList"/>
        <typeAlias alias="Users" type="com.myspringboot.demo.model.User"/>
        <typeAlias alias="Masters" type="com.myspringboot.demo.model.Master"/>
        <typeAlias alias="Roleuser" type="com.myspringboot.demo.model.RoleUser"/>
        <typeAlias alias="Permissions" type="com.myspringboot.demo.model.Permissions"/>
        <typeAlias alias="Roles" type="com.myspringboot.demo.model.Role"/>
        <typeAlias alias="Rolespermissions" type="com.myspringboot.demo.model.RolePermissions"/>
        <typeAlias alias="Classes" type="com.myspringboot.demo.model.Classes"/>
        <typeAlias alias="Schools" type="com.myspringboot.demo.model.Schools"/>
        <typeAlias alias="Teachers" type="com.myspringboot.demo.model.Teachers"/>
        <typeAlias alias="Subjects" type="com.myspringboot.demo.model.Subjects"/>
        <typeAlias alias="Parents" type="com.myspringboot.demo.model.Parents"/>
        <typeAlias alias="Grades" type="com.myspringboot.demo.model.Grades"/>
        <typeAlias alias="Attendance" type="com.myspringboot.demo.model.Attendance"/>
    </typeAliases>

    <!-- Other settings go here -->
</configuration>
